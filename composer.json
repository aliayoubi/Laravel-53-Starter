{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.6.4",
    "laravel/framework": "5.3.*",
    "rap2hpoutre/laravel-log-viewer": "^0.7.0",
    "yajra/laravel-datatables-oracle": "~6.0",
    "davejamesmiller/laravel-breadcrumbs": "^3.0",
    "watson/active": "^2.0",
    "laravelbook/ardent": "^3.4",
    "itsgoingd/clockwork": "^1.12",
    "eusonlito/laravel-packer": "^2.1",
    "uxweb/sweet-alert": "^1.3",
    "laracasts/flash": "^2.0",
    "sarfraznawaz2005/visitlog": "^1.0",
    "hemp/presenter": "^1.0",
    "recca0120/laravel-tracy": "^1.7",
    "themsaid/laravel-mail-preview": "^1.2",
    "laracasts/utilities": "^2.1",
    "iatstuti/laravel-cascade-soft-deletes": "^1.1",
    "rinvex/repository": "^2.0",
    "malhal/laravel-createdby": "^1.0"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.0",
    "symfony/css-selector": "3.1.*",
    "symfony/dom-crawler": "3.1.*",
    "barryvdh/laravel-ide-helper": "^2.2"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/Support/helper.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta",
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
